/*! layout */
.ma-layout-wrap
    padding 40px
    background-color #f6f6f6

.ma-layout
    .ma-layout-col-left
        float left
        width 25%
        min-width 200px
        padding-right 20px
        text-align center

    .ma-layout-col-right
        float left
        width 75%
        padding 0 20px

.ma-white-wrap
    background-color #fff
    margin-bottom 20px
    padding 20px 32px
    box-shadow 0 2px 2px 0 rgba(0, 0, 0, 0.14), 0 3px 1px -2px rgba(0, 0, 0, 0.2), 0 1px 5px 0 rgba(0, 0, 0, 0.12)

    .section-heading
        margin 24px 0

    h1
        margin-top 0
        padding-top 10px